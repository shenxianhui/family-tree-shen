<view class="home">
  <image
    class="home-bg"
    src="../../assets/img/bg.jpg"
    mode="aspectFill"
    lazy-load="false"
    binderror=""
    bindload=""
  >
  </image>
  <view class="info">
    <!-- <view class="title" style="{{titleStyle}}">申氏族谱</view> -->
    <view class="title">申氏族谱</view>
    <view class="name">
      <text class="name-item">申贤慧</text>
      <text class="name-item">著</text>
    </view>
  </view>
  <view class="menu">
    <div class="menu-item" bindtap="handleCover" data-menu-idx="{{2}}">碧霄</div>
    <div class="menu-item main" bindtap="handleCover" data-menu-idx="{{1}}">族谱</div>
    <div class="menu-item" bindtap="handleCover" data-menu-idx="{{3}}">琳琅</div>
  </view>

	<view class="bird-wrap">
    <view class="sky">
      <view class="bird">
        <view class="wind"></view>
        <view class="wind"></view>
        <view class="wind"></view>
        <view class="wind"></view>
        <view class="wind"></view>
        <view class="wind"></view>
        <view class="wind"></view>
        <view class="wind"></view>
        <view class="wind"></view>
        <view class="wind"></view>
        <view class="bird_body">
          <view class="bird_head"></view>
          <view class="bird_wing_left">
            <view class="bird_wing_left_top"></view>
          </view>
          <view class="bird_wing_right">
            <view class="bird_wing_right_top"></view>
          </view>
          <view class="bird_tail_left"></view>
          <view class="bird_tail_right"></view>
        </view>
      </view>
    </view>
    <view class="sky sky2">
      <view class="bird2">
        <view class="wind"></view>
        <view class="wind"></view>
        <view class="wind"></view>
        <view class="wind"></view>
        <view class="wind"></view>
        <view class="wind"></view>
        <view class="wind"></view>
        <view class="wind"></view>
        <view class="wind"></view>
        <view class="wind"></view>
        <view class="bird_body">
          <view class="bird_head"></view>
          <view class="bird_wing_left">
            <view class="bird_wing_left_top"></view>
          </view>
          <view class="bird_wing_right">
            <view class="bird_wing_right_top"></view>
          </view>
          <view class="bird_tail_left"></view>
          <view class="bird_tail_right"></view>
        </view>
      </view>
    </view>
	</view>
</view>

